    "@babel/register": "^7.0.0-beta.34",
    "eslint": "^4.12.0",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.7.0",
    "mocha": "^4.0.1",
    "nodemon": "^1.12.1",
    "prettier-eslint-cli": "^4.1.1"
  },
  "dependencies": {
    "aplay": "^1.2.0",
    "express": "^4.16.2",
    "sox": "^0.1.0",
    "vantage": "^1.7.0"
